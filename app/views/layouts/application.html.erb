<!DOCTYPE html>
<html>
  <head>
    <title>RailsYelpMvp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="nav">
      <%= link_to "Back", restaurants_path(@restaurant) %>
      <%= link_to "New", new_restaurant_path(@restaurant) %>
    </div>
    <div>
      <%= yield %>
    </div>
  </body>
</html>
